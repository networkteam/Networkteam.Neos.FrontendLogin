    <file original="" product-name="Networkteam.Neos.FrontendLogin" source-language="en" target-language="de" datatype="plaintext">
        <body>
            <trans-unit id="components.atoms.loginform.username" xml:space="preserve">
                <source>Username</source>
                <target>Benutzername</target>
            </trans-unit>
            <trans-unit id="components.atoms.loginform.password" xml:space="preserve">
                <source>Password</source>
                <target>Passwort</target>
            </trans-unit>
            <trans-unit id="components.atoms.loginform.submit" xml:space="preserve">
                <source>Login</source>
                <target>Anmelden</target>
            </trans-unit>
            <trans-unit id="components.atoms.account.logout" xml:space="preserve">
                <source>Logout</source>
                <target>Abmelden</target>
            </trans-unit>
            <trans-unit id="components.atoms.account.statusMessage" xml:space="preserve">
                <source><![CDATA[You're logged in as <strong>"{0}"</strong>]]></source>
                <target><![CDATA[Du bist als <strong>"{0}"</strong> angemeldet]]></target>
            </trans-unit>
            <trans-unit id="authentication.onAuthenticationFailure.authenticationFailed.title" xml:space="preserve">
                <source>Authentication failed!</source>
                <target>Anmeldung fehlgeschlagen!</target>
            </trans-unit>
            <trans-unit id="authentication.onAuthenticationFailure.redirectFailed.title" xml:space="preserve">
                <source>Redirect failed!</source>
                <target>Weiterleitung fehlgeschlagen!</target>
            </trans-unit>
            <trans-unit id="authentication.onAuthenticationFailure.redirectFailed.body" xml:space="preserve">
                <source>Exception occurred while trying to redirect: {exceptionMessage} ({exceptionCode})</source>
                <target>Während der Weiterleitung ist ein Fehler aufgetreten: {exceptionMessage} ({exceptionCode})</target>
            </trans-unit>
            <trans-unit id="authentication.onAuthenticationFailure.authenticationFailed.body" xml:space="preserve">
                <source>The login could not be performed with the given credentials.</source>
                <target>Mit den eingegebenen Zugangsdaten konnte die Anmeldung nicht durchgeführt werden.</target>
            </trans-unit>
            <trans-unit id="loginhandler.inbackend" xml:space="preserve">
                <source>Form is disabled in backend mode.</source>
                <target>Das Formular is im Backend-Modus deaktiviert.</target>
            </trans-unit>

            <!-- nodeTypes -->

            <trans-unit id="nodeTypes.groups.frontendLogin" xml:space="preserve">
                <source>Member area</source>
                <target>Mitgliederbereich</target>
            </trans-unit>
            <trans-unit id="nodeTypes.groups.frontendLogin.actions" xml:space="preserve">
                <source>Actions</source>
                <target>Aktionen</target>
            </trans-unit>
        </body>
